}

.list-item {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  margin: 0 30rpx 23rpx 30rpx;
  padding: 0 0 0 25rpx;
  background-color: #f5f5f5;
  border-radius: 5px;
