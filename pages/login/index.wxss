}

.list-item .l-l {
  height: 88rpx;
  line-height: 88rpx;
  width: 100%;
}

.input {
  margin-left: 20rpx;
  font-size: 28rpx;
}

.loginBtn {
  height: 90rpx;
  line-height:  90rpx;
  margin: 64rpx 30rpx 0 30rpx;
  color: white;
  font: 30rpx;
}

.checkBox {
  zoom: 60%;
  align-self: center;
  padding-top: 15rpx;
}

.botBtn {
  padding-top: 15rpx;
  font-size: 26rpx;
  color: #2f2f2f;
}

/* base64  backset*/

.account::before, .password::before {
  content: "";
  width: 28rpx;
  height: 33rpx;
  display: block;
  float: left;
}

.account::before, .password::before, icon[type="clear"] {
  background-position: center center;
  background-repeat: no-repeat;
  background-size: contain;
  position: relative;
  z-index: 13;
}

icon[type="clear"] {
  background-image: url('data:image/png;base64,');
}

.account::before {
  background-image: url('data:image/png;base64,');
}

.password::before {
  background-image: url('data:image/png;base64,');
}
